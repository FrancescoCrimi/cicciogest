<?xml version="1.0" encoding="utf-8"?>
<configuration>

  <configSections>
    <section name="DddTest" type="Ciccio1.Infrastructure.Conf.DddTestConfigurationSection, Infrastructure" />
    <!--<section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />-->
    <section name="nlog" type="NLog.Config.ConfigSectionHandler, NLog" />
  </configSections>

  <DddTest>
    <Confs default="mysql">
      <Conf name="mysql" user_interface="WCF" data_access="NHibernate">
        <data_access_config database="MySql" cs="server=localhost;User Id=dddtest;password=dddtest;database=ciccio1" />
      </Conf>
      <Conf name="ssee1" user_interface="WCF" data_access="NHibernate">
        <data_access_config database="SSEE" cs="Data Source=FRANCESCO-PC\SQLEXPRESS;Initial Catalog=test;Integrated Security=True" />
      </Conf>
      <Conf name="ssee2" user_interface="WCF" data_access="NHibernate">
        <data_access_config database="SSEE" cs="Data Source=(LocalDB)\v11.0;AttachDbFilename=|DataDirectory|\DddTest.mdf;Integrated Security=True" />
      </Conf>
      <Conf name="ssce" user_interface="WCF" data_access="NHibernate">
        <data_access_config database="SSCE" cs="Data Source=DddTest.sdf" />
      </Conf>
      <Conf name="db4o" user_interface="WCF" data_access="Db4o">
        <data_access_config database="MySql" cs="DddTest.db4o" />
      </Conf>
    </Confs>
  </DddTest>

  <nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <targets>
      <target xsi:type="NLogViewer" name="suca" onOverflow="Split" address="udp://127.0.0.1:8765" includeSourceInfo="true" includeCallSite="true" includeNdc="true" includeNLogData="true" includeMdc="true" />
      <target xsi:type="Debugger" name="vsd" />
      <target xsi:type="File" name="file" fileName="log.txt" />
    </targets>

    <rules>
      <logger name="*" minlevel="Debug" writeTo="suca" />
    </rules>
  </nlog>

