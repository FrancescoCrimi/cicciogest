<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping assembly="Domain"
                   namespace="Ciccio1.Domain"
                   xmlns="urn:nhibernate-mapping-2.2">

  <class name="Dettaglio" table="Dettaglio">
    
    <id name="Id" generator="native" unsaved-value="0"/>
    <property name="Quantità" not-null="true" />
    <many-to-one name="Prodotto" lazy="false" column="Prodotto_Id" not-null="true" />
    
  </class>

</hibernate-mapping>
