<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="Domain"
                   namespace="Ciccio1.Domain"
                   xmlns="urn:nhibernate-mapping-2.2">

  <class name="Prodotto" table="Prodotto">
    
    <id name="Id" generator="native" unsaved-value="0"/>
    <property name="Nome" not-null="true"/>
    <property name="Prezzo" not-null="true"/>
    <many-to-one name="Categoria" lazy="false" column="Categoria_Id" not-null="true"/>
    
  </class>

</hibernate-mapping>
